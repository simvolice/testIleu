


        <!-- BEGIN PROFILE CONTENT -->
        <div class="profile-content">
            <div class="row">
                <div class="col-md-12">
                    <div class="portlet light">
                        <div class="portlet-title tabbable-line">
                            <div class="caption caption-md">
                                <i class="icon-globe theme-font hide"></i>
                                <span class="caption-subject font-blue-madison bold uppercase">Старт нового процесса</span>
                            </div>

                        </div>
                        <div class="portlet-body">
                            <div class="tab-content">
                                <!-- PERSONAL INFO TAB -->
                                <div class="tab-pane active" id="tab_1_1">
                                    <form role="form" action="/processpost" method="post" enctype="multipart/form-data" onsubmit="return postForm()">






                                        <div class="form-group">
                                            <label class="control-label">Тип процесса</label>


                                            <select id="type" name="type">

                                              {{#each typeprocess}}


                                                  <option value="{{id}}">{{nameType}}</option>

                                              {{/each}}





                                            </select>


                                        </div>





                                        <div class="form-group">
                                            <label class="control-label">Наименование процесса</label>


                                            <select id="name" name="name" class="form-control">


                                                  {{#each nameprocess}}




                                                        <option value="{{idname}}" class="{{id}}">{{name}}</option>




                                                {{/each}}



                                            </select>


                                        </div>



                                      <input type="hidden" name="connectprocess">
                                      <input type="hidden" name="primarydoc">


                                        <div class="form-group" style="margin-top: 40px">
                                            <label class="control-label">Таблица</label>
                                            <a href="#portlet-config" class="btn" data-toggle="modal" style="margin-left: 80%; background: #DDDDDD">Работа со справочником</a>




                                            <input name="rowfromtable" type="hidden">
                                            <input name="headertable" type="hidden">


                                            <table id="newtable" style="border-collapse: collapse;">


                                                <thead>



                                                <tr id="tableforname">



                                                </tr>



                                                </thead>


                                                <tbody id="forvalue">











                                                </tbody>




                                            </table>





                                        </div>









                                        <div class="form-group">
                                            <label class="control-label">Дата и время окончания процесса</label>
                                            <div class="">
                                                <div class="input-group date form_datetime">
                                                    <input type="text" name="datetime" size="16" readonly class="form-control">
												<span class="input-group-btn">
												<button class="btn default date-set" type="button"><i class="fa fa-calendar"></i></button>
												</span>
                                                </div>

                                            </div>
                                        </div>


                                        <div class="form-group">
                                            <label class="control-label">Исполнитель</label>


                                            <select name="performer" class="bs-select form-control" data-live-search="true" data-size="auto" multiple>




                                              {{#each employees}}



                                                    <option value="{{id}}">{{displayname}}</option>



                                              {{/each}}



                                            </select>


                                        </div>



                                        <div class="form-group">
                                            <label class="control-label">Ответственные лица</label>


                                            <select name="answerable" class="bs-select form-control" data-live-search="true" data-size="8" multiple>



                                              {{#each employees}}



                                                    <option value="{{id}}">{{displayname}}</option>


                                              {{/each}}





                                            </select>


                                        </div>





                                        <div class="form-group form-md-line-input form-md-floating-label">

                                            <label for="summernote">Текст процесса</label>
                                            <textarea id="summernote" name="content" rows="18" class="form-control"></textarea>

                                        </div>




                                        <div class="form-group">


                                            <div class="fileinput fileinput-new" data-provides="fileinput">
                                                <div class="input-group input-large">
                                                    <div class="form-control uneditable-input input-fixed input-medium" data-trigger="fileinput">
                                                        <i class="fa fa-file fileinput-exists"></i>&nbsp; <span class="fileinput-filename">
														</span>
                                                    </div>
													<span class="input-group-addon btn default btn-file">
													<span class="fileinput-new">
													Выберети документ </span>
													<span class="fileinput-exists">
													Изменить </span>
													<input type="file" name="document" multiple>
													</span>
                                                    <a href="javascript:;" class="input-group-addon btn red fileinput-exists" data-dismiss="fileinput">
                                                        Удалить </a>
                                                </div>
                                            </div>

                                        </div>









                                        <div class="margiv-top-10">
                                            <button type="submit" class="btn green-haze">
                                                Стартануть процесс </button>

                                        </div>
                                    </form>
                                </div>
                                <!-- END PERSONAL INFO TAB -->

                                <!-- END CHANGE AVATAR TAB -->


                                <!-- PRIVACY SETTINGS TAB -->

                                <!-- END PRIVACY SETTINGS TAB -->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- END PROFILE CONTENT -->



        <div id="portlet-config" class="modal container fade" tabindex="-1">




            <div class="modal-body">



                <div class="profile-content">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="portlet light">
                                <div class="portlet-title tabbable-line">
                                    <div class="caption caption-md">
                                        <i class="icon-globe theme-font hide"></i>
                                        <span class="caption-subject font-blue-madison bold uppercase">Работа со справочником</span>
                                    </div>
                                    <ul class="nav nav-tabs">
                                        <li class="active">
                                            <a href="#tab_1_1" data-toggle="tab">Добавить запись</a>
                                        </li>


                                    </ul>
                                </div>
                                <div class="portlet-body">
                                    <div class="tab-content">
                                        <!-- PERSONAL INFO TAB -->
                                        <div class="tab-pane active" id="tab_1_1">
                                            <div id="formsave">













                                            </div>


                                            <div class="margiv-top-10">
                                                <button id="savenew" class="btn green-haze">
                                                    Сохранить запись </button>

                                            </div>

                                        </div>



                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>















            </div>

        </div>








